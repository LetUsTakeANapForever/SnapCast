import os
import cv2
import matplotlib.pyplot as plt
from PIL import Image
import numpy as np
import xlsxwriter


img_out = cv2.imread('quantized_image4.png')



def cl_histrogram(img):
    h, w = img.shape[:2]
    his = np.full(256, 0, dtype=int)
    for i in range(h):
        for j in range(w):
            his[img[i][j][0]] += 1
    return his


x = range(256)
histrogram = cl_histrogram(img_out)
plt.bar(x, histrogram)
plt.show()

